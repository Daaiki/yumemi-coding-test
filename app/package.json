{
  "name": "app",
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "dev": "yarn dev:next",
    "dev:next": "next dev",
    "dev:scss": "typed-scss-modules src --watch",
    "build:next": "next build && next export",
    "lint": "next lint -d ./ --ignore-path .gitignore",
    "lint:fix": "next lint -d ./ --fix --ignore-path .gitignore",
    "lint:warnings": "next lint -d ./ --fix --ignore-path .gitignore --max-warnings=0",
    "check:type": "tsc --pretty --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "check:format": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "export": "next export",
    "typegen:scss": "typed-scss-modules src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -s ./public -o ./build/storybook/",
    "test": "jest",
    "json-server": "json-server --watch ./src/mocks/db.json --routes ./src/mocks/routes.json --port 4000 --host 0.0.0.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "highcharts": "^10.3.1",
    "highcharts-react-official": "^3.1.0",
    "next": "^13.0.4",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "sass": "^1.56.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-docs": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-storyshots": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "babel-loader": "^8.3.0",
    "eslint": "^8.28.0",
    "eslint-config-next": "^13.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-storybook": "^0.6.7",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "json-server": "^0.17.1",
    "msw": "^0.49.0",
    "prettier": "^2.7.1",
    "sass-loader": "^10.0.0",
    "ts-jest": "^29.0.3",
    "typed-scss-modules": "^7.0.1",
    "typescript": "^4.9.3"
  },
  "resolutions": {
    "enhanced-resolve": "5.10.0",
    "react-test-renderer": "18.2.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
